package com.mycompany.user.services;

import java.util.List;

import com.mycompany.user.entities.User;

public interface UserService {
	
	//User Operation
	
	//create 
	User saveUser(User user);
	
	//get all user
	List<User> getAllUser();
	
	//get single user of given user Id
		User getUser(String userId);
		
	//Update user's details of given user Id
		User putUser(String userId);
		
	//Delete user's details of given user Id
		User deleteUser(String userId);

}
